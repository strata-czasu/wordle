{
  "name": "@hashira/wordle",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "start:prod": "bun prisma-migrate-deploy && NODE_ENV=production bun seed && NODE_ENV=production bun src/index.tsx",
    "seed": "bun run seed.ts",
    "fix": "biome check --write",
    "typecheck": "tsc --build",
    "prisma-studio": "prisma studio --port 5556",
    "prisma-generate": "prisma generate",
    "prisma-check-migrations": "prisma migrate deploy && prisma migrate diff --exit-code --from-schema-datamodel ./prisma/ --to-schema-datasource ./prisma/",
    "prisma-migrate-dev": "prisma migrate dev",
    "prisma-migrate-deploy": "prisma migrate deploy",
    "prisma-migrate-reset": "prisma migrate reset",
    "prisma-push": "prisma db push"
  },
  "dependencies": {
    "@discord/embedded-app-sdk": "^2.1.0",
    "@discordjs/rest": "^2.5.1",
    "@prisma/client": "^6.13.0",
    "bun-plugin-tailwind": "^0.0.14",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.16",
    "es-toolkit": "^1.36.0",
    "jose": "^6.0.12",
    "prisma": "^6.13.0",
    "prisma-json-types-generator": "^3.5.1",
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4.0.6",
    "valibot": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tsconfig/bun": "^1.0.7",
    "@tsconfig/strictest": "^2.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "^1.2.19",
    "lefthook": "^1.12.2",
    "typescript": "^5.8.3"
  }
}
